<deviceinfo version="0.2">
  <device>
    <match key="info.capabilities" contains="input.mouse">
      <match key="input.originating_device" contains="i8042_AUX_port">      
        <merge key="input.x11_driver" type="string">prlmouse</merge>
      </match>
    </match>
  </device>
</deviceinfo>
